                                                        <option value="36">36 KVA</option>
                                                        <option value="36"> +36 KVA</option>
                                                    </select>


                                                </div>
                                                <div class="form-group md-3 mt-2"> <label for="input6" class="form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Support de poteau
                                                        <br>(Point de livraison, le plus proche.) <b>*</b> :<br></label>
                                                    <input type="text" class="form-control form-control-sm" id="input6" maxlength="6" name="PL" aria-describedby="input1_help" placeholder="000000" required="" data-msg-required="- Ce champ est requis." oninput="validateInput(this)">
                                                </div>

                                                <div class="form-group md-3 mt-2"> <label class="form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Nombre de compteur :</label>
                                                    <input type="text" maxlength="2" class="form-control form-control-sm" aria-describedby="input1_help" disabled="true" dmx-bind:value="1">
                                                </div>

                                                <div class="form-group md-3 mt-2"> <label for="input7" class="form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Nombre de climatiseur <b>*</b> :</label>
                                                    <input type="text" maxlength="2" class="form-control form-control-sm" id="input7" name="nbr_clim" aria-describedby="input1_help" required="" data-msg-required="- Ce champ est requis." oninput="validateInput(this)">
                                                </div>
                                                <div class="form-group md-3 mt-2"> <label for="input8" class="form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Nombre de pièce éclairée <b>*</b> :</label>
                                                    <input type="text" maxlength="2" class="form-control form-control-sm" id="input8" name="Nbr_eclaire" aria-describedby="input1_help" required="" data-msg-required="- Ce champ est requis." oninput="validateInput(this)">
                                                </div>
                                                <div class="form-group md-3 mt-2"> <label for="input9" class="form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Nombre de réfrigérateur <b>*</b> :</label>
