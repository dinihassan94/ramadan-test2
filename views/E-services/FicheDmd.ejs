                        <h5 class="mt-1 text-center fw-bold text-white"><i class="fas fa-step-forward">&nbsp;&nbsp;</i><span id="stepIndicator">Etape 1 sur 3</span></h5>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col">
                                <form id="multiStepForm" method="post" is="dmx-serverconnect-form" action="/api/Enregistrement/InsertionDemandeWeb" dmx-on:success="ModalDemandeEffectuer.show()">


                                    <div id="step1" class="form-step">

                                        <div class="row">
                                            <div class="col">
                                                <div class="form-group md-3 small">
                                                    <label for="input1" class="form-label col-form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Groupe clientèle <b>*</b> :</label><select id="select1" class="form-select form-select-sm" dmx-bind:options="GroupeClientele.data" optiontext="lib_long" dmx-on:changed="Statut.load();Usage.load({});Modele_doc_affich.reset()" optionvalue="id_ligne" name="GroupeClientele">

                                                        <option value="">--- Votre choix ---</option>
                                                    </select>
                                                </div>
                                                <div class="form-group md-3 small mt-2">
                                                    <label for="input1" class="form-label col-form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Statut&nbsp;<b>*</b> :</label><select id="select2" class="form-select form-select-sm" optiontext="lib_long" optionvalue="id_ligne" dmx-bind:options="StatutArray.items.where(`id_parent`, select1.value, '==')" required="" data-msg-required="- Ce champ est requis." name="Statut">

                                                    </select>
                                                </div>

                                                <div class="form-group md-3 small mt-2">
                                                    <label for="input1" class="form-label col-form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Usage <b>*</b> :</label><select id="select3" class="form-select form-select-sm" dmx-bind:options="UsageArray.items.where(`id_parent`, select1.value, '==')" optiontext="lib_long" required="" data-msg-required="- Ce champ est requis." optionvalue="id_ligne" dmx-on:changed="Modele_doc_affich.reset();Modele_doc.load()" name="Usage">

                                                        <option value=""></option>
                                                    </select>
                                                </div>


                                                <div class="form-group md-3 small mt-2">
                                                    <label for="input1" class="form-label col-form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Type logement&nbsp; <b>*</b> :</label><select id="select9" class="form-select form-select-sm" dmx-bind:options="TypeLogement.data" optiontext="lib_long" required="" data-msg-required="- Ce champ est requis." optionvalue="id_ligne" dmx-on:changed="Modele_doc_affich.reset();Modele_doc.load()" name="Type_Logement">

                                                        <option value="0">TYPE NON DÉFINIS</option>
                                                    </select>
                                                </div>
                                                <div class="form-group md-3 small mt-2">
                                                    <label for="input1" class="form-label col-form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Type d'intervention <b>*</b> :</label><select id="select7" class="form-select form-select-sm" dmx-bind:options="TypeIntervention.data" optiontext="lib_long" required="" data-msg-required="- Ce champ est requis." optionvalue="id_ligne" dmx-on:changed="Modele_doc_affich.reset();Modele_doc.load()" name="Type_Intervention">
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        <button class="btn text-white mt-5 ps-4 pe-4 fw-bold btn-sm shadow-sm" type="button" dmx-on:click="run([{run:{outputType:'text',action:`select2.validate();select3.validate();select9.validate();select7.validate()`}},{wait:{delay:200}},{condition:{outputType:'boolean',if:`select3.invalid`,then:{steps:[{'toast.setup':{position:'bottom',delay:2000}},{'toast.show':{message:'Veuillez remplir correctement pour accéder à la page suivante',title:'&lt;span style=&quot;color: red&quot;&gt;&lt;svg xmlns=&quot;http://www.w3.org/2000/svg&quot; width=&quot;15&quot; height=&quot;15&quot; fill=&quot;currentColor&quot; class=&quot;bi bi-exclamation-triangle-fill&quot; viewBox=&quot;0 0 16 16&quot;&gt;  &lt;path d=&quot;M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5m.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2&quot;/&gt;&lt;/svg&gt; - Alerte&lt;/span&gt;',subtitle:'Validation',icon:'/public/assets/images/arton142.png'}}]},else:{steps:{runJS:{outputType:'text',function:'showStep',args:['2']}}}}}])" dmx-style:background-color="'#005eb8'">Suivant&nbsp;&nbsp;<i class="fas fa-caret-right"></i></button>
                                    </div>

                                    <!-- Step 2 -->
                                    <div id="step2" class="form-step" style="display:none;">

                                        <div class="row">
                                            <div class="col">

                                                <div class="form-group md-3 small">
                                                    <label for="input1" class="form-label col-form-label"><i class="fas fa-arrow-alt-circle-right">&nbsp;&nbsp;</i>Modèle document <b>*</b> :</label><select id="select8" class="form-select form-select-sm" required="" data-msg-required="- Ce champ est requis." dmx-on:changed="Modele_doc_affich.reset();Modele_doc_affich.load()" optiontext="modele" optionvalue="num_ligne" name="Modele_document" dmx-bind:options="select9.value==0?Model_doc_array.items:Model_doc_array.items.where(`type_logement`, select9.value, '==')">

                                                        <option value="">--- Votre choix ---</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        <!-- Your Step 2 fields -->
                                        <div class="row">
                                            <div class="col mt-3">
                                                <div class="card shadow-sm">
                                                    <div class="card-header fw-bold shadow-sm text-primary"><i class="fas fa-file-alt">&nbsp;</i> Documents requis</div>
                                                    <div class="card-body">
                                                        <div class="row">
                                                            <div class="col rounded small">



                                                                <table class="table table-bordered table-hover">
                                                                    <thead>
                                                                    </thead>
